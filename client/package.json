{
    "name": "client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite && VITE_ENVIRONMENT=development",
        "build": "tsc && vite build",
        "embed": "tsc && vite build && cp -r dist ../server/client/",
        "sync": "aws s3 sync ./dist s3://farmec.ie/",
        "invalidate": "aws cloudfront create-invalidation --distribution-id EGIJU0OVG257E --paths '/*'",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "deploy": "npm run build && npm run sync && npm run invalidate",
        "local": "VITE_ENVIRONMENT=development npm run dev"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@react-google-maps/api": "^2.19.3",
        "@stripe/react-stripe-js": "^2.4.0",
        "@stripe/stripe-js": "^2.4.0",
        "@tanstack/react-query": "^5.17.19",
        "firebase": "^10.7.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-router-dom": "^6.21.3",
        "typewriter-effect": "^2.21.0",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/react-helmet": "^6.1.11",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "@typescript-eslint/parser": "^8.30.1",
        "@vitejs/plugin-react-swc": "^3.9.0",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "postcss": "^8.4.33",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vite": "6.2.6",
        "vite-plugin-sitemap": "^0.7.1"
    }
}
